<%= form_with url: posts_path, method: :post do%>
  <div>
    <%= label_tag :post_title, "Title" %>
    <%= text_field_tag :post_title %>
  </div>
  <div>
    <%= label_tag :post_body, "Body" %>
    <%= text_area_tag :post_body %>
  </div>
  <div>
    <%= submit_tag "Create Post" %>
  </div>
<% end %>